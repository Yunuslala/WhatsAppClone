// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  picture String 
  About String
  relations relations[]
}


model relations{
  id Int @id @default(autoincrement())
  user User @relation(fields: [reciverId],references: [id])
  reciverId Int 
  senderId Int 
  conversations Json
}

